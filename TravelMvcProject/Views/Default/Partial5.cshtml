@using TravelMvcProject.Models.Siniflar
@model List<Blog>
<div class="box1-left">
    @foreach (var blog in Model)
    {
        <div class="bot-img">
            <img src="@blog.BlogImage" style="width:120px; height:65px;"  alt="" />
        </div>
        <div class="bot-desc1">
            <p>@blog.Aciklama.Substring(0,70)...</p>
        </div>
        <div class="clear"></div>
    }
    </div>


