@using TravelMvcProject.Models.Siniflar
@model List<Blog>
<div class="padding">
    <h4>En Popüler 10 Blog</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var blog in Model)
        {
            <li class="list-bg"><span>@{i++;}@i.</span><a href="/Blog/BlogDetay/@blog.ID">@blog.Baslik</a></li>
        }

    </ul>
    <a href="/Blog/Index/" class="link1">Tüm Blogları Gör.</a>
</div>
